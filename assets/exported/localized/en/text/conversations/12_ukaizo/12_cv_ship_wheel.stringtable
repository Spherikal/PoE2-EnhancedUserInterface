    <Entry>
      <ID>2</ID>
      <DefaultText>[Sail to Ukaizo.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>[Make additional preparations.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>

      <FemaleText />
    </Entry>
    <Entry>
      <ID>1037</ID>
      <DefaultText>Vytmádh</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1038</ID>
      <DefaultText>An Ordhjóma term translating to "White Maw."</DefaultText>
      <FemaleText />
    </Entry>
